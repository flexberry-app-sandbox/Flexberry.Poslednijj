<h3 class="ui header">{{t "forms.i-i-s-poslednijj-отчет-о-зак-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номер
      class=(if (v-get validationModel "номер" "isInvalid") "error")
      label=(t "forms.i-i-s-poslednijj-отчет-о-зак-e.номер-caption")
      data-test-i-i-s-poslednijj-отчет-о-зак-e-номер=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номер" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-poslednijj-отчет-о-зак-e.датаНачала-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.датаНачала
      class=(if (v-get validationModel "датаНачала" "isInvalid") "error")
      data-test-i-i-s-poslednijj-отчет-о-зак-e-датаНачала=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "датаНачала" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-poslednijj-отчет-о-зак-e.датаОкончания-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.датаОкончания
      class=(if (v-get validationModel "датаОкончания" "isInvalid") "error")
      data-test-i-i-s-poslednijj-отчет-о-зак-e-датаОкончания=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "датаОкончания" "message")}}
  </div>
  <div class="field" data-test-i-i-s-poslednijj-отчет-о-зак-e-состОтгрузки="true">
    <label>{{t "forms.i-i-s-poslednijj-отчет-о-зак-e.состОтгрузки-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-poslednijj-сост-поставок")
      value=model.состОтгрузки
      class=(if (v-get validationModel "состОтгрузки" "isInvalid") "error")
      readonly=readonly
    }}
    {{flexberry-validationmessage error=(v-get validationModel "состОтгрузки" "message")}}
  </div>
  <div class="field" data-test-i-i-s-poslednijj-отчет-о-зак-e-состояниеОплаты="true">
    <label>{{t "forms.i-i-s-poslednijj-отчет-о-зак-e.состояниеОплаты-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-poslednijj-сост-оплаты")
      value=model.состояниеОплаты
      class=(if (v-get validationModel "состояниеОплаты" "isInvalid") "error")
      readonly=readonly
    }}
    {{flexberry-validationmessage error=(v-get validationModel "состояниеОплаты" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-poslednijj-отчет-о-зак-e.суммаВклНДС-caption"}}</label>
    {{flexberry-checkbox
      readonly=readonly
      value=model.суммаВклНДС
      class=(if (v-get validationModel "суммаВклНДС" "isInvalid") "error")
      data-test-i-i-s-poslednijj-отчет-о-зак-e-суммаВклНДС=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "суммаВклНДС" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.суммаДокумента
      class=(if (v-get validationModel "суммаДокумента" "isInvalid") "error")
      label=(t "forms.i-i-s-poslednijj-отчет-о-зак-e.суммаДокумента-caption")
      data-test-i-i-s-poslednijj-отчет-о-зак-e-суммаДокумента=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "суммаДокумента" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-poslednijj-отчет-о-зак-e.тЧЗаказ-caption"}}</label>
    {{flexberry-groupedit
      componentName="тЧЗаказGroupEdit"
      mainModelProjection=modelProjection
      modelProjection=modelProjection.attributes.тЧЗаказ
      content=model.тЧЗаказ
      readonly=readonly
      orderable=false
      class=(if (v-get validationModel "тЧЗаказ" "isInvalid") "error")
      data-test-i-i-s-poslednijj-отчет-о-зак-e-тЧЗаказ=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "тЧЗаказ" "message")}}
  </div>
</form>
